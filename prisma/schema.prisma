generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model leads {
  id           String   @id @default(uuid())
  created_at   DateTime @default(now())
  email        String   @unique
  phone_number BigInt?
  zip_code     String
  pwsid        String?
}



model Contaminant_Mapping {
  id                     String   @id @default(uuid())
  pws_id                 String
  zip_code               String?
  water_system_name      String?
  detected_patriots_count Int
  report_data            Json     
  klaviyo_event_sent     Boolean  @default(false)
  created_at             DateTime @default(now())
  
  @@map("contaminant_mapping")
  @@unique([pws_id, zip_code])
}
